{
  "expo": {
    "name": "GoJimApp",
    "slug": "GoJimApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/playstoreIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/playstoreIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstoreIcon.png",
        "backgroundColor": "transparent"
      },
      "package": "com.jimpesenti.GoJimApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/playstoreIcon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "43f85dc9-e6aa-4c3f-80f0-a49b99df197a"
      }
    }
  }
}
